<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>

<h2>Song Name: <%=@song.name%></h2>
<h2>Song Artist: <%=@song.artist.name%></h2>
<p><a href="/artists/<%=@song.artist.slug%>">Details</a></p></h2>
<h2>Song Genres: </h2>
  <% @song.genres.each do |genre| %>
      <%= genre.name %>
      <p><a href="/genres/<%=genre.slug%>">Details</a></p>
  <% end %>
